{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": [
            ">1%",
            "last 4 versions",
            "Firefox ESR",
            "not ie < 9"
          ]
        }
      }
    ],
    "stage-0"
  ],
  "plugins": [
    "transform-async-to-generator",
    "transform-runtime",
    "transform-decorators-legacy",
  ],
  "env": {
    "test": {
      "plugins": [
        "dynamic-import-node",
//        "rewire"
      ]
    }
  }
}
