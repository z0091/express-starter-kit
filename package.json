  "scripts": {
    "start": "node server/index.js",
    "start:prod": "node server/index.js --release",
    "build": "$(npm bin)/babel-node utils/run build",
    "build:dist": "npm run build -- --release",
    "bundle:app": "$(npm bin)/babel-node utils/run bundleApp",
    "bundle:server": "$(npm bin)/babel-node utils/run bundleServer",
    "clean": "$(npm bin)/babel-node utils/run clean",
    "lint": "$(npm bin)/eslint .",
    "security:nsp": "$(npm bin)/nsp check",
    "security:snyk": "$(npm bin)/snyk test",
    "security": "npm run security:nsp && npm run security:snyk",
    "unit": "$(npm bin)/karma start --single-run",
    "e2e": "$(npm bin)/babel-node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e && npm run security && npm run lint"
  },
